    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>

    <project-components>
        <component>
            <implementation-class>gitpairpicker.GitPairProjectComponent</implementation-class>
        </component>
    </project-components>
</idea-plugin>